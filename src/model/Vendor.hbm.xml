<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="model">
<class name="Vendor" table="vendor">
	<id name="vendorId" column="VENDOR_ID">
	<generator class="assigned"></generator>
	</id>
	<property name="vendorName" column="vendorName"></property>
	<property name="vendorAddress" column="vendorAddress"></property>
	<property name="vendorEmail" column="vendorEmail"></property>
	</class>
	
</hibernate-mapping>